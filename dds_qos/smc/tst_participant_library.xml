<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="@CONNEXTDDS_RESOURCE_QOS_PROFILES_SCHEMA_DIR@/rti_dds_qos_profiles.xsd"
    version="@CONNEXTDDS_FOUND_VERSION@">

    <!-- This domain participant file is used for testing. -->

    <participant_library name="MscSmcParticipantLibrary">
    
        <!-- Realtime SMC application domain participant -->
        <domain_participant name="MscApplicationRealtimeParticipant" domain_id="@SMC_REALTIME_DOMAIN_ID@">

            <!-- Topics using keep last reliable QoS profile -->

            <!-- Topics published by SMC -->

            <!-- State  -->
            <!-- Actual -->
            <register_type name="SmcStateActual" kind="userGenerated"></register_type>
            <topic register_type_ref="SmcStateActual" name="SMC/SmcStateActual"></topic>
            <data_reader topic_ref="SMC/SmcStateActual" name="SmcStateActualReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datareader_qos>
            </data_reader>
            <data_writer topic_ref="SMC/SmcStateActual" name="SmcStateActualWriter">
                <datawriter_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datawriter_qos>
            </data_writer>

            <!-- SwitchControls  -->
            <register_type name="SwitchControls" kind="userGenerated"></register_type>
            <topic register_type_ref="SwitchControls" name="SMC/SwitchControls"></topic>
            <data_reader topic_ref="SMC/SwitchControls" name="SwitchControlsReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datareader_qos>
            </data_reader>
            <data_writer topic_ref="SMC/SwitchControls" name="SwitchControlsWriter">
                <datawriter_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datawriter_qos>
            </data_writer>

            <!-- Topics received by SMC -->

            <!-- State  -->
            <!-- Teleoperation  -->
            <register_type name="SmcStateTeleoperation" kind="userGenerated"></register_type>
            <topic register_type_ref="SmcStateTeleoperation" name="MSC/SmcStateTeleoperation"></topic>
            <data_writer topic_ref="MSC/SmcStateTeleoperation" name="SmcStateTeleoperationWriter">
                <datawriter_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datawriter_qos>
            </data_writer>
            <data_reader topic_ref="MSC/SmcStateTeleoperation" name="SmcStateTeleoperationReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datareader_qos>
            </data_reader>

            <!-- SmcHandleNotification  -->
            <register_type name="SmcHandleNotification" kind="userGenerated"></register_type>
            <topic register_type_ref="SmcHandleNotification" name="MSC/SmcHandleNotification"></topic>
            <data_writer topic_ref="MSC/SmcHandleNotification" name="SmcHandleNotificationWriter">
                <datawriter_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datawriter_qos>
            </data_writer>
            <data_reader topic_ref="MSC/SmcHandleNotification" name="SmcHandleNotificationReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::SmcKeyedReliableQoS"></datareader_qos>
            </data_reader>


            <!-- Topics using best effort QoS profile -->

            <!-- ForceFeedbackCommand, received by SMC -->
            <register_type name="ForceFeedbackCommand" kind="userGenerated"></register_type>
            <topic register_type_ref="ForceFeedbackCommand" name="MSC/ForceFeedbackCommand"></topic>
            <data_writer topic_ref="MSC/ForceFeedbackCommand" name="ForceFeedbackCommandWriter">
                <datawriter_qos base_name="EinsteinQoSProfileLibrary::EinsteinRealtimeControlData"></datawriter_qos>
            </data_writer>
            <data_reader topic_ref="MSC/ForceFeedbackCommand" name="ForceFeedbackCommandReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::EinsteinRealtimeControlData"></datareader_qos>
            </data_reader>

            <!-- HandleData, published by SMC  -->
            <register_type name="HandleData" kind="userGenerated"></register_type>
            <topic register_type_ref="HandleData" name="SMC/HandleData"></topic>
            <data_reader topic_ref="SMC/HandleData" name="HandleDataReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::EinsteinRealtimeControlData"></datareader_qos>
            </data_reader>
            <data_writer topic_ref="SMC/HandleData" name="HandleDataWriter">
                <datawriter_qos base_name="EinsteinQoSProfileLibrary::EinsteinRealtimeControlData"></datawriter_qos>
            </data_writer>

            <!-- DebugMsc, published by MSC  -->
            <register_type name="DebugMsc" kind="userGenerated"></register_type>
            <topic register_type_ref="DebugMsc" name="MSC/DebugMsc"></topic>
            <data_reader topic_ref="MSC/DebugMsc" name="DebugMscReader">
                <datareader_qos base_name="EinsteinQoSProfileLibrary::EinsteinRealtimeControlData"></datareader_qos>
            </data_reader>

        </domain_participant>

    </participant_library>
</dds>
